{
    "manifest_version": 3,
    "name": "Task Manager Extension",
    "version": "1.0",
    "description": "A simple task manager Chrome extension.",
    "permissions": [
      "storage",
      "tabs"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
      }
    
  }
  